{
  "app": "npx ts-node bin/network.ts",
  "context": {
    "@aws-cdk/core:enableStackNameDuplicates": "true",
    "aws-cdk:enableDiffNoFail": "true",
    "@aws-cdk/core:stackRelativeExports": "true",
    "AZs": ["ap-notheast-2a","ap-northeast-2c"],
    "prj": "ATCL",
    "stage": "DEV",
    "DEV": {
      "nat_gateways": 2
    },
    "SHARED": {
      "nat_gateways": 1
    },
    "STAGE": {
      "nat_gateways": 2
    },
    "PROD": {
      "nat_gateways": 2
    }
  }
}
